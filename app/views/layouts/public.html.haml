!!!
%html
  %head
    %title= page_title
    = stylesheet_link_tag    "application", :media => "all"
    = javascript_include_tag "application"
    = csrf_meta_tags
  %body
    #wrapper
      #header
        = render "shared/header"
      = render "shared/notices"
      .row
        #navbar
          = render "shared/navbar"
        #content
          = yield

      #footer
        = render "shared/footer"
